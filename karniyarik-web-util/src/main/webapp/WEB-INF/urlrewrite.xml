<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">

<urlrewrite decode-using="UTF-8" >

	<rule>
        <from>^/img/(.+)$</from>
        <to last="true">/img</to>
    </rule>

	<rule>
        <from>^/imgrsz/(.+)$</from>
        <to last="true">/imgrsz</to>
    </rule>
</urlrewrite>